<h1>Create a company</h1>

<br />
<div class="col-xs-10 col-xs-offset-1">
  <h1>Add a new Company!</h1>
  <%= simple_form_for @company, :url => new_company_path do |f| %>
      <%= f.input :name %>
      <br />
      <%= f.submit "Create", :class => 'btn btn-primary' %>
      <br />
  <% end %>
  <br />
</div>